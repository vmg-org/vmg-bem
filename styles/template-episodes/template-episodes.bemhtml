block('template-episodes')(
  tag()('div'),
  content()(
    this.ctx.arr.map(function(mv) {
      return {
        elem: 'item',
        content: mv
      }
    })
  ),
  elem('item')(
    content()(function() {
      return {
        block: 'episode-info',
        content: [{
          elem: 'title',
          content: applyNext().title
        }, {
          elem: 'story',
          content: applyNext().story
        }]
      };
    })
  )
);
